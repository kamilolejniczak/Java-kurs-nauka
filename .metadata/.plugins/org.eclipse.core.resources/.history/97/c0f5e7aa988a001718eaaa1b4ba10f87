package app;
	


import java.io.IOException;

import javafx.application.Application;
import javafx.fxml.FXMLLoader;
import javafx.scene.Parent;
import javafx.scene.Scene;
import javafx.stage.Stage;




public class Main extends Application {
	@Override
	public void start(Stage stageGame){
		try {
			Parent parent = FXMLLoader.load(getClass().getResource("/app/view/statkiVew.fxml"));
			Scene sceneGame = new Scene(parent);
			stageGame.setScene(sceneGame);
			stageGame.setTitle("Statki by Kamil");
			stageGame.setResizable(false);
			stageGame.show();
	} catch (IOException e) {
			e.printStackTrace();
		}
		}
	
	
	public static void main(String[] args) {
		launch(args);
	}
}
